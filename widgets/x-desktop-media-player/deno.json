{
  "tasks": {
    "build": "deno run -A --node-modules-dir=manual npm:vite build",
    "load": "seelen-ui resource load widget ./dist",
    "unload": "seelen-ui resource unload widget ./dist",
    "dev": "deno task unload && deno task build -- --dev && deno task load"
  },
  "imports": {
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.5",
    "@sveltejs/vite-plugin-svelte": "npm:@sveltejs/vite-plugin-svelte@^6.1.3",
    "svelte": "npm:svelte@^5.38.3",
    "vite": "npm:vite@^7.1.3"
  }
}